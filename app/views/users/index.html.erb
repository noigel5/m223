<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>username</th>
      <th>email</th>
      <th>address</th>
      <th>telephone number</th>
      <th>actions</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.address %></td>
        <td><%= user.telnr %></td>
        <td>
          <%= link_to "Edit", edit_admin_path(user) %>
        </td>
        <td>
          <%= link_to "Delete", admin_path(user), data: { turbo_confirm: "Really delete this user?", turbo_method: :delete} %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
